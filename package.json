{
  "name": "linkedin-manager",
  "version": "0.6.1",
  "private": true,
  "main": "public/main.js",
  "author": "Murali",
  "description": "My fantastic Electron app",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/Murali1889/Linkedin-Manager"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^5.16.1",
    "@mui/material": "^5.16.4",
    "@mui/x-tree-view": "^7.11.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.19.3",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "electron-updater": "^6.2.1",
    "firebase": "^10.12.5",
    "franken-ui": "^0.2.0",
    "fs": "^0.0.1-security",
    "google-auth-library": "^9.11.0",
    "googleapis": "^140.0.1",
    "lucide-react": "^0.414.0",
    "path": "^0.12.7",
    "postcss": "^8.4.43",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-scripts": "5.0.1",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.5",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "web-vitals": "^2.1.4",
    "zod": "^3.23.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "electron-builder",
    "publish": "electron-builder --publish always",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=public/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron:build:mac": "yarn build && electron-builder --mac",
    "electron:build:win": "yarn build && electron-builder --win",
    "electron:build:all": "yarn build && electron-builder -mw",
    "electron:publish": "yarn electron:build:all --publish always",
    "electron:build:start": "npm run build && npm run electron:serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "tailwindcss": "^3.4.6",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.electron.myapp",
    "productName": "Linkedin Manager",
    "directories": {
      "output": "dist",
      "buildResources": "public"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "public/main.js"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Murali1889",
        "repo": "Linkedin-Manager",
        "releaseType": "release",
        "overwrite": true,
        "publishAutoUpdate": true
      }
    ]
  }
}
